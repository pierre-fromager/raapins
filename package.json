{
    "name": "raapins",
    "author": "Pierre Fromager",
    "version": "1.0.1",
    "licence": "GNU GPLv3",
    "repository": "git@github.com:pierre-fromager/raapins.git",
    "contributors": [
        "Pierre Fromager"
    ],
    "type": "module",
    "dependencies": {
        "async": "^3.2.2",
        "chai": "^4.2.0",
        "dotenv": "^8.2.0",
        "mocha": "^8.2.0",
        "newman": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/node": "^7.12.1",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/register": "^7.12.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "jsdoc": "^3.6.6",
        "nodemon": "^2.0.6"
    },
    "keywords": [
        "restful",
        "async",
        "api",
        "node",
        "server"
    ],
    "scripts": {
        "watch": "nodemon --exec babel-node src/server.js",
        "test": "mocha --require @babel/register 'src/**/**spec.js'",
        "doc": "jsdoc -P ./package.json -R ./README.md -r -d ./doc --debug ./src",
        "lint": "node_modules/.bin/eslint --fix src/*.js",
        "server": "node src/server.js --experimental-modules",
        "client": "node src/client.js"
    }
}
